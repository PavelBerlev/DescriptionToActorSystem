﻿<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Форма</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SpaApp.styles.css" asp-append-version="true" />
</head>
<body>
    <header class="header container">
        <a class="header__logo" asp-area="" asp-controller="Home" asp-action="Index">Загрузка работ</a>
    </header>
    <main role="main container" class="main">
        @RenderBody()
    </main>
    <footer class="footer container">
        &copy; 2024 - PavelKriko
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
